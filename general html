 <script type="text/javascript"> 
 function handleClick() 
 { var amountCorrect = 0; 
 for(var i = 1; i <= 50; i++) 
 { var radios = document.getElementsByName('group'+i);
 for(var j = 0; j < radios.length; j++) 
 { var radio = radios[j]; 
 if(radio.value == "correct" && radio.checked) { amountCorrect++; } } } alert("Correct Responses: " + amountCorrect);
 } 
 </script>
